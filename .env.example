# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Ray configuration
# Leave empty to start a new local Ray instance
# Set to "auto" to connect to a local Ray cluster
# Set to "ray://<host>:<port>" to connect to a remote Ray cluster
RAY_ADDRESS=

# Kodosumi environment flag
# This is set automatically by Kodosumi when deployed
# KODOSUMI_ENVIRONMENT=true

# Other API keys
# Add your API keys here
# EXAMPLE_API_KEY=your_example_api_key

# Exa.ai API Key for web search
EXA_API_KEY=your_exa_api_key

# Ray configuration
RAY_TASK_NUM_CPUS=0.1       # CPU resources allocated per Ray task (fractional values allowed)
RAY_TASK_MAX_RETRIES=3      # Maximum number of retries for Ray tasks
RAY_TASK_TIMEOUT=10.0       # Timeout in seconds for Ray task execution
RAY_BATCH_SIZE=1            # Number of insights to process in each batch
RAY_INIT_NUM_CPUS=2         # Number of CPUs to allocate when initializing a new Ray instance
RAY_DASHBOARD_PORT=None     # Port for Ray dashboard (use "None" to disable)

# Other environment variables
KODOSUMI_ENVIRONMENT=false  # Set to "true" when running in Kodosumi environment 