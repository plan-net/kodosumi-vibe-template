# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key

# Ray configuration
# Leave empty to start a new local Ray instance
# Set to "auto" to connect to a local Ray cluster
# Set to "ray://<host>:<port>" to connect to a remote Ray cluster
RAY_ADDRESS=

# Kodosumi environment flag
# This is set automatically by Kodosumi when deployed
# KODOSUMI_ENVIRONMENT=true

# Other API keys
# Add your API keys here
# EXAMPLE_API_KEY=your_example_api_key

# Exa.ai API Key for web search
EXA_API_KEY=your_exa_api_key 